@using Shared.Models

<div class="card shadow mb-2">
    <div class="card-body">
        <h3 class="class-title">@Ghc.Name</h3>
        <p class="card-text">@Ghc.Full_Name</p>
        <p class="card-text">@Ghc.Language</p>
        <p class="card-text">@Ghc.Created_At</p>
        <p class="card-text">@Ghc.Updated_At</p>
        <p class="card-text">@Ghc.Visibility</p>
        <p class="card-text">@Ghc.Owner.Login</p>
    </div>
</div>

@code 
{
 [Parameter] public GithubRepos Ghc { get; set; }
}
